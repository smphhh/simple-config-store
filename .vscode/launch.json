{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch server",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/main.ts",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"DEBUG": "*",
				"AWS_PROFILE": "gio-development-user"
			},
			"externalConsole": false,
            "preLaunchTask": "build-shell",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/lib"
		},
		{
			"name": "Launch shell",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/server/src/shell.ts",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/server",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"DEBUG": "*",
				"AWS_PROFILE": "gio-development-user"
			},
			"externalConsole": true,
            "preLaunchTask": "build-server",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/server/lib"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		},
		{
			"name": "Run mocha",
			"type": "node",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": ["lib/**/__tests__/**/*.js"],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": "build",
			"request": "launch",
			"runtimeExecutable": null,
			"env": {
				"NODE_ENV": "production",
				"DEBUG": "",
				"AWS_PROFILE": "gio-development-user"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/lib"
		}
	]
}
